{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals": {
    "process": false
  },
  "rules": {
    "no-console": 0,
    "quotes": 0,
    "curly": 0,
    "new-cap": 0,
    "camelcase": 0,
    "no-unused-vars": 0,
    "comma-dangle": 1,
    "no-extra-semi": 1,
    "key-spacing": 1,
    "semi-spacing": 1,
    "eol-last": 1,
    "valid-jsdoc": 1,
    "no-undef": 2
  }
}
